{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "usage",
        "corejs": {
          "version": "3.19",
          "proposals": true
        }
      }
    ],
    [
      "@babel/react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@app": "./src/app",
          "@routes": "./src/routes",
          "@styles": "./src/styles",
          "@features": "./src/features"
        }
      }
    ]
  ]
}
